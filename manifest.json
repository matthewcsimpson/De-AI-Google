{
  "manifest_version": 3,
  "name": "De-AI Google",
  "version": "1.0.0",
  "description": "Appends \"-ai\" once and forces classic Web results.",
  "permissions": ["declarativeNetRequestWithHostAccess"],
  "host_permissions": [
    "*://*.google.com/*",
    "*://*.google.co.uk/*",
    "*://*.google.ca/*",
    "*://*.google.de/*",
    "*://*.google.fr/*",
    "*://*.google.it/*",
    "*://*.google.es/*",
    "*://*.google.com.au/*",
    "*://*.google.co.jp/*",
    "*://google.com/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*",
        "*://*.google.co.uk/*",
        "*://*.google.ca/*",
        "*://*.google.de/*",
        "*://*.google.fr/*",
        "*://*.google.it/*",
        "*://*.google.es/*",
        "*://*.google.com.au/*",
        "*://*.google.co.jp/*",
        "*://google.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "declarative_net_request": {
    "rule_resources": [{ "id": "rules", "enabled": true, "path": "rules.json" }]
  }
}
